<div ng-controller="SearchDetailController" ng-init="initDataFirstTime('<%= tenquan %>' ,'<%= tendichvu %>','<%= ngay %>','<%= giobd %>','<%= giokt %>')" class="container wrapper">
  <title>ProFixer - Tìm kiếm ngắn hạn</title>
  <div ng-show="isSearch" class="row content">
    <div class="col-lg-3 content-left">
      <md-toolbar class="md-whiteframe-2dp">
        <div class="md-toolbar-tools">
          <h4 class="chutrang"> <i class="fa fa-search"></i>  Tìm kiếm</h4>
        </div>
      </md-toolbar>
      <md-content flex="" layout-padding="" class="md-whiteframe-2dp">
        <div class="form-inline xanhnhat">
          <md-subheader><i class="fa fa-calendar padright xanhnhat"> </i>Ngày<i class="fa fa-warning pull-right padright xanhnhat">
              <md-tooltip md-direction="left">Thay đổi ngày sẽ mất các lựa chọn hiện tại</md-tooltip></i></md-subheader>
          <div>
            <input id="ngay" type="text" name="ngay" placeholder="Chọn ngày" readonly="readonly" autocomplete="off" ng-model="data.ngay" ng-change="initData()" class="marleft form-control timewidth dpindex"/>
          </div>
          <div>
            <p class="red">{{thongbaongay}}</p>
          </div>
        </div>
        <div class="form-inline xanhnhat">
          <md-subheader><i class="fa fa-clock-o padright xanhnhat"> </i>Giờ<i class="fa fa-warning pull-right padright xanhnhat">
              <md-tooltip md-direction="left">Thay đổi giờ sẽ mất các lựa chọn hiện tại</md-tooltip></i></md-subheader>
        </div>
        <div class="form-inline">
          <div class="form-group">
             <select ng-model="data.giobd" class="form-control sb fixWTime nya-selectpicker">
                        <option ng-repeat="gio in dsGio" ng-value="gio.name">{{gio.name}}</option>
                      </select>
           <select ng-model="data.giokt" class="form-control sb fixWTime nya-selectpicker">
                        <option ng-repeat="gio in dsGio" ng-value="gio.name">{{gio.name}}</option>
                      </select>
          </div>
          <div>
            <p class="red">{{thongbaogio}}</p>
          </div>
        </div>
        <!-- số năm kinh nghiệm ----------------------------------------------------->
        <div class="form-inline">
          <div class="xanhnhat">
            <md-subheader><i class="fa fa-briefcase padright xanhnhat"> </i>Kinh Nghiệm</md-subheader>
          </div>
          <md-radio-group ng-model="data.sonamkn">
            <div ng-repeat="kn in kinhnghiems">
              <md-radio-button value="{{kn.id}}">{{kn.ten}}</md-radio-button>
            </div>
          </md-radio-group>
        </div>
        <!-- dịch vụ ----------------------------------------------------->
        <div class="form-inline">
          <div class="xanhnhat">
            <md-subheader><i class="fa fa-tags padright xanhnhat"> </i>Dịch vụ</md-subheader>
          </div>
          <div layout="row" layout-align="center center" ng-if="loading_dichvu"><i class="fa fa-2x fa-cog fa-spin xanhnhat"></i></div>
<!--
          <div ng-repeat="dichvu in dichvus">
            <md-radio-group ng-model="searchData.tendichvu" >{{dichvu.tendichvu}}</md-radio-group>
          </div>
-->
                      <md-radio-group ng-model="searchData.tendichvu">
            <div ng-repeat="dichvu in dichvus">
              <md-radio-button value="{{dichvu.tenDichVu}}">{{dichvu.tenDichVu}}</md-radio-button>
            </div>
          </md-radio-group>
        </div>
        <!-- Quận ----------------------------------------------------->
        <div class="form-inline ">
          <div class="xanhnhat">
            <md-subheader><i class="fa fa-map-marker padright xanhnhat"> </i>Quận<i class="fa fa-warning pull-right padright xanhnhat">
                <md-tooltip md-direction="left">Thay đổi quận sẽ mất các lựa chọn hiện tại</md-tooltip></i></md-subheader>
          </div>
          <div class="my-scroll">
          <md-radio-group ng-model="searchData.tenquan">
            <div ng-repeat="quan in quans">
              <md-radio-button value="{{quan.tenquan}}">{{quan.tenquan}}</md-radio-button>
            </div>
          </md-radio-group>
          </div>
        </div>
        <input id="submitnh1" type="submit" ng-click="Search()" value="Tìm kiếm" class="form-control sb btn btn-danger">
      </md-content>
    </div>
    <div class="col-lg-9 content-right">
      <h3 class="md-headline headline">
        <div layout="row" layout-align="center center">
          <div flex="50" layout="row" layout-align="start center"><i class="fa fa-list xanhdam">  Danh sách người sửa chữa   </i>
            <md-button aria-label="Use Android" class="md-fab md-primary chutrang">
              <md-tooltip>{{'Bạn đã chọn ' + ngv_selected_arr.length + ' nhân viên!' }}</md-tooltip><i class="fa fa-user"></i>  {{ngv_selected_arr.length}}
            </md-button>
          </div>
          <div flex="50" layout="row" layout-align="end center">
            <button type="button" ng-click="show_yeucau()" class="btn btn-danger pull-right"><i class="fa fa-shopping-cart"></i> Yêu cầu sửa chữa</button>
          </div>
        </div>
      </h3>
      <div layout="row" layout-align="center center" ng-show="loading"><i class="fa fa-4x fa-cog fa-spin xanhnhat"></i></div>
      <div class="removedot" ng-hide="loading">
        <ul>
          <li ng-repeat="ngv in ngvs">
            <md-card>
              <md-card-content>
                <div class="row selected-classifieds">
                  <div class="col-md-3"><img ng-click="chon_ngv(ngv)" ng-src="ngv.hinhanh" alt="Ảnh đại diện" class="img-responsive"/>
                      <md-tooltip>Bấm vào hình để chọn người sửa chữa.</md-tooltip>
                    <div class="rating"><i class="fa fa-2x fa-star xanhnhat"></i><i class="fa fa-2x fa-star xanhnhat"></i><i class="fa fa-2x fa-star xanhnhat"></i><i class="fa fa-2x fa-star-o xanhnhat"></i><i class="fa fa-2x fa-star-o xanhnhat"></i></div>
                  </div>
                  <div class="col-md-9">
                    <div id="{{ngv.cmnd}}" class="row selected-classifieds marbtm">
                      <div class="col-md-12">
                        <h2 class="headline ngv_name">{{ngv.hoten}}</h2>
                      </div>
                    </div>
                    <hr class="hrnomargin"/>
                    <div class="rightbody">
                      <div class="row vdivide paddingrow">
                        <div class="col-md-6">
                          <h4 class="i fa fa-paperclip padright xanhnhat"> </h4>{{ngv.danhgia}}
                        </div>
                        <div class="col-md-6">
                          <div class="row">
                            <div class="itembody left">
                              <h4 class="i fa fa-venus-mars padright xanhnhat"> </h4>{{ngv.gioitinh}}
                            </div>
                            <div class="itembody right">
                              <h4 class="i fa fa-birthday-cake padright xanhnhat"></h4>{{(ngv.ngaysinh)}}
                            </div>
                          </div>
                          <div class="row">
                            <div class="itembody left">
                              <h4 class="i fa fa-briefcase padright xanhnhat"></h4>{{ngv.sonamkinhnghiem + ' năm kinh nghiệm'}}
                            </div>
                            <div class="itembody right">
                              <h4 class="i fa fa-map-marker padright xanhnhat"></h4>{{ngv.diachi.quan}}
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="form-group">
                      <h4 class="i fa fa-tags padright xanhnhat"></h4>Sở trường<br/>
                      <input ng-repeat="st in ngv.sotruong" type="button" value="{{st}}" class="btn btn-danger btn-xs tagmargin"/>
                    </div>
                    <hr class="hrnomargin"/>
                    <div class="rightfooter">
                      <button type="button" ng-click="show_detail(ngv)" class="btn btn-danger pull-right"><i class="fa fa-arrow-right"></i> Thông tin chi tiết</button>
                    </div>
                  </div>
                </div>
              </md-card-content>
            </md-card>
          </li>
        </ul>
      </div>
    </div>
  </div>
