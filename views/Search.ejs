                  <input disabled="disabled" value="{{searchData.giokt}}"/>
                </md-input-container>
              </div>
              <div layout="column" layout-sm="column">
                <md-input-container style="width:25%">
                  <label> <a class="i fa fa-clock-o xanhnhat padright"></a>Dịch vụ</label>
                  <input disabled="disabled" value="{{searchData.tendichvu}}"/>
                </md-input-container>
              </div>
              <div layout="" layout-sm="column">
                <md-input-container style="width:50%">
                  <label> <a class="i fa fa-money xanhnhat padright"></a>Phí cơ bản</label>
                  <input disabled="disabled" value="{{dichvu1.phiTheoGio+'Đ/Giờ'}}"/>
                </md-input-container>
              </div>
              <div layout="" layout-sm="column">
                <md-input-container style="width:50%">
                  <label> <a class="i fa fa-money xanhnhat padright"></a>Tổng chi phí</label>
                  <input disabled="disabled" value="{{searchData.tongchiphi+'Đ'}}"/>
                </md-input-container>
              </div>
              <div layout="row" layout-sm="column">
                <md-list ng-cloak="ng-cloak" style="width:100%">
                  <p>Nhân viên đã chọn.</p>
                  <md-divider></md-divider>
                  <md-list-item ><img alt="Ảnh đại diện" ng-src="ngscDcChon.hinhanh" class="md-avatar"/>
                    <p>{{ngscDcChon.hoten}}</p>
                  </md-list-item>
                </md-list>
              </div>
            </md-content>
          </div>
          <div ng-if="!loading_yeucau &amp;&amp; !hoanthanh_thanhtoan_nh" class="modal-footer">
            <button type="submit" ng-disabled="formthanhtoan.$invalid" ng-click="luu_yeucau()" class="marleft btn btn-danger pull-right"><i class="fa fa-check"></i> Đồng ý</button>
            <button type="button" ng-click="close_thanhtoan()" class="marleft btn btn-danger pull-right"><i class="fa fa-remove"></i> Hủy</button>
          </div>
          <div ng-if="hoanthanh_thanhtoan_nh" class="modal-footer">
            <button type="button" ng-click="hoanthanh_thanhtoan(1)" class="marleft btn btn-danger pull-right"><i class="fa fa-check"></i> Xem lịch sử đơn hàng</button>
            <button type="button" ng-click="hoanthanh_thanhtoan(2)" class="marleft btn btn-danger pull-right"><i class="fa fa-check"></i> Về trang chủ</button>
          </div>
        </form>
      </div>
    </div>
